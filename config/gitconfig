[alias]
    a = add -A
    ca = commit -a --amend --no-edit
    c = commit -am
    d = diff
    dt = difftool
    p = push origin master
    pu = pull origin master
    r = remote
    s = status
[color]
    ui = true
[core]
    editor = nvim
    sparsecheckout = true
[diff]
    prompt = false
    tool = vimdiff
[difftool]
    path = nvim
[hub]
    protocol = ssh
