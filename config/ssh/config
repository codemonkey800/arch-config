Host aur
    HostName aur.archlinux.org
    IdentityFile ~/.ssh/aur
    User aur

Host github.com gitlab.com bitbucket.com
    HostName %h
    IdentityFile ~/.ssh/vcs
    User git

Host desktop
    HostName desktop.vpn.jeremyasuncion.io
    IdentityFile ~/.ssh/vps
    RemoteCommand fish
    RequestTTY yes

Host laptop
    HostName laptop.vpn.jeremyasuncion.io
    IdentityFile ~/.ssh/vps
    RemoteCommand fish
    RequestTTY yes

Host prod
    HostName vpn.jeremyasuncion.io
    IdentityFile ~/.ssh/vps
    User root

