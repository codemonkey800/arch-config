{
  "aria2": {
    "args": [
      "--conf-path",
      "/etc/pacman.d/pacman-aria2.conf"
    ],
    "path": "/usr/bin/aria2c"
  },
  "pacman": {
    "config": "/etc/pacman.conf",
    "path": "/usr/bin/pacman"
  },
  "pacserve": {
    "server": null
  },
  "powerpill": {
    "select": true,
    "reflect databases": false
  },
  "reflector": {
    "args.unused": [
        "--country",
        "US",
        "--sort",
        "rate"
    ]
  },
  "rsync": {
    "args": [
      "-a",
      "-P",
      "--no-motd"
    ],
    "db only": true,
    "path": "/usr/bin/rsync",
    "servers": []
  }
}
